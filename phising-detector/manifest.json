{
  "manifest_version": 3,
  "name": "Phishing Detector",
  "version": "1.0",
  "description": "Checks the current URL for phishing risk using a machine learning model.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "http://127.0.0.1:8000/"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}